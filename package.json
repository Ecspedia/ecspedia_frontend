{
  "name": "ecspedia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "start:dev": "next build --turbopack && next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --headed --project=chromium",
    "prepare": "husky install",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^4.0.8",
    "@hookform/resolvers": "^5.2.2",
    "@next/swc-wasm-nodejs": "^16.0.1",
    "@reduxjs/toolkit": "^2.9.0",
    "@types/react-window": "^1.8.8",
    "@vis.gl/react-google-maps": "^1.5.5",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.12.0",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.65.0",
    "react-redux": "^9.2.0",
    "react-window": "^2.2.3",
    "rxjs": "^7.8.2",
    "stripe": "^12.18.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-scrollbar-hide": "^4.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.15.1",
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "6.0.2",
    "@graphql-codegen/client-preset": "5.1.3",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@parcel/watcher": "^2.5.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^4.0.6",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "husky": "^8.0.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.6"
  }
}
